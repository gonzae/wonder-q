{
  "name": "wonder-q",
  "version": "1.0.0",
  "description": "A simple queue service",
  "main": "dist/app.tjs",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "start:dev": "npx ts-node src/start.ts",
    "start": "node dist/start.js",
    "test": "jest --verbose ./src",
    "test:w": "jest --watch ./src"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.8"
